/dts-v1/;


/ {
	compatible = "Tsinghua,NaiveMIPS,Thinpad";
	#address-cells = <1>;
	#size-cells = <1>;

	chosen {
		bootargs = "console=ttyS0,9600n8";
	};

	cpus {
		#address-cells = <1>;
		#size-cells = <0>;
		cpu@0 {
			device_type = "cpu";
			compatible = "mips,4Kc";
			clocks	= <&ext>;
			reg = <0>;
		};
		cpu@1 {
			device_type = "cpu";
			compatible = "mips,4Kc";
			clocks	= <&ext>;
			reg = <1>;
		};
	};

	ext: ext {
		compatible = "fixed-clock";
		#clock-cells = <0>;
	};

	memory {
		device_type = "memory";
		reg = <0x0 0x8000000>;
	};

	cpuintc: interrupt-controller@0 {
		#address-cells = <0>;
		#interrupt-cells = <1>;
		interrupt-controller;
		compatible = "mti,cpu-interrupt-controller";
	};
	
	soc{
		compatible = "simple-bus";
		#address-cells = <1>;
		#size-cells = <1>;
		#interrupt-cells = <1>;
		ranges;
		
		serial0: serial@0x1fd003F8 {
			device_type = "serial";
			compatible = "tsinghua,naivemips-uart";
			reg = <0x1fd003F8 8>;
			clock-frequency = <153600>;
			interrupt-parent = <&cpuintc>;
			interrupts = <4>;
		};

	};

	
};

&ext {
	clock-frequency = <25000000>;
};
